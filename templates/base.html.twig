		<title>
			{% block title %}Wassa - Bienvenue!
			{% endblock %}
		</title>

		{#feuille de styles #}
		{% block stylesheet %}

			{# #} <link href="{{asset('assets/css/bootstrap.min.css')}}" rel="stylesheet"/>
			<link href="{{ asset('css/jquery/jquery-ui.min.css') }}" rel="stylesheet"/>
			<link href="{{ asset('css/jquery/jquery-ui.theme.min.css') }}" rel="stylesheet"/>
			<link href="{{ asset('css/jquery/jquery-ui.structure.min.css') }}" rel="stylesheet"/>
			<link href="{{ asset('css/bootstrap/bootstrap.4.3.1.min.css') }}" rel="stylesheet"/>
