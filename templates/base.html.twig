			<script src="{{ asset('js/jquery/jquery.3.3.1.min.js') }}"></script>
			<script src="{{ asset('js/jquery/jquery-ui.js') }}"></script>
			<script src="{{ asset('js/jquery/jquery.validate.min.js') }}"></script>
			<script src="{{ asset('js/popper.1.14.7.min.js') }}"></script>
			<script src="{{ asset('js/bootstrap/bootstrap.4.3.1.min.js') }}"></script>
			<script src="{{ asset('js/pnotify/jquery.pnotify.min.js') }}"></script>
			<script src="{{ asset('js/main.js') }}"></script>
			<script src="{{ asset('js/wassaJS/dialogs.js') }}"></script>
			<script src="{{ asset('js/wassaJS/notifications.js') }}"></script>
		{% endblock %}
